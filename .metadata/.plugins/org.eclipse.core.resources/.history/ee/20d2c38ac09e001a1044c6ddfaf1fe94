
public class StringUpdates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str="";
		System.out.println(str.isBlank());
		String str2=" ";
		System.out.println(str.isBlank());
		String str3=" aa";
		System.out.println(str.isBlank());

	}

}
/*
 * true true true
 */