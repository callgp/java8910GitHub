package com.gp.stringapi;

public class StringFeatures2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s="all the power there.do it please";
		System.out.println(s);
		
		System.out.println("after indentation:"+s.indent(5));
		
		System.out.println("after minust indentation:"+s.indent(-2));//it takes out spaces
		System.out.println("after indentation of -50:"+s.indent(-50));//even give minus 50 no error just removes it

	}

}
/*
 * all the power there.do it please after indentation all the power there.do it
 * please
 */

